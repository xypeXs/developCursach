            INSERT INTO GOOD_TYPES(GOOD_TYPE_ID, NAME) VALUES(100, 'Стройматериалы - доски');
            INSERT INTO GOOD_TYPES(GOOD_TYPE_ID, NAME) VALUES(101, 'Шуруповёрты');
